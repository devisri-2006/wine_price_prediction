        "    def __init__(self, name, marks):\n",
        "        self.name = name\n",
        "        self.marks = marks\n",
        "\n",
        "    def result(self):\n",
        "        if self.marks >= 40:\n",
        "            return \"Pass\"\n",
        "        else:\n",
        "            return \"Fail\"\n"
